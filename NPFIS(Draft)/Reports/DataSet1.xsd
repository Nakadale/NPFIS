                    <CommandText>SELECT        LoanTransaction.TransactCode, LoanTransaction.EmpId, LoanTransaction.ApplicationNum, LoanTransaction.DateFiled, LoanTransaction.ChequeNum, LoanTransaction.ChequeDate, LoanTransaction.DateRelease,
